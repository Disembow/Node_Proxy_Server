{
  "name": "node_proxy_server",
  "version": "1.0.0",
  "description": "Node Proxy Server study project",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=production ts-node --esm index.ts",
    "dev": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development tsc index.ts",
    "format": "prettier -w ./app",
    "lint": "eslint ./app",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js, jsx, ts, tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "keywords": [],
  "author": "Yauheni Naliotau",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.8",
    "body-parser": "^1.20.3",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "nunjucks": "^3.2.4",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/nunjucks": "^3.2.6",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
